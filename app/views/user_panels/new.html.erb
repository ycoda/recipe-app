<h1>UserPanel#new</h1>
<p>Find me in app/views/user_panel/new.html.erb</p>


<% provide(:title, 'user_panel') %>
<h1>user_panel</h1>

<div class="row">
  <div class="span6 offset3">

    <%= form_for @user_panel, :html => { :multipart => true }  do |f| %>
      <%# render 'shared/error_messages' %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :ingredient %>
      <%= f.text_field :ingredient %>

      <%= f.label :seasoning %>
      <%= f.text_field :seasoning %>

      <%= f.label :picture %>
      <%= f.file_field :picture %>

      <%= f.submit "post", class: "btn btn-large btn-primary" %>
  <% end %>
  </div>
</div>
